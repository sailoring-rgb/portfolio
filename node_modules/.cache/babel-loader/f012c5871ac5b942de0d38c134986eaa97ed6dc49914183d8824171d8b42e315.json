{"ast":null,"code":"//import AOS from \"aos\";\n//import \"aos/dist/aos.css\";\n\nimport HomePage from './components/HomePage.vue';\nexport default {\n  name: 'App',\n  components: {\n    HomePage\n  }\n};","map":{"version":3,"names":["HomePage","name","components"],"sources":["/Users/annaphens/portfolio/src/App.vue"],"sourcesContent":["<template>\n  <div>\n    <nav class=\"navbar fixed-top navbar-expand-lg\" style=\"\">\n      <div class=\"container-fluid\">\n        <a href=\"#home\">\n          <img src=\"./assets/logo.png\" style=\"width:50px;align-items: center;\">\n        </a>\n        <a class=\"navbar-brand\" href=\"#about\">ABOUT</a>\n        <a class=\"navbar-brand\" href=\"#background\">BACKGROUND</a>\n        <a class=\"navbar-brand\" href=\"#projects\">PROJECTS</a>\n        <a class=\"navbar-brand\" href=\"#contacts\">CONTACTS</a>\n        <p></p>\n      </div>\n    </nav>\n  </div>\n\n  <HomePage/>\n\n  <div class=\"card-container\">\n    <div v-if=\"!isMinimized\">\n      <div class=\"card d-flex justify-content-center align-items-center shadow\">\n        <p class=\"music-title\">{{ Object.keys(songTitles[currentSongIndex])[0] }}</p>\n        <p class=\"music-author\">{{ Object.values(songTitles[currentSongIndex])[0] }}</p>\n\n        <div class=\"progress-container\">\n          <div class=\"progress-bar\" ref=\"progressBar\"></div>\n          <div class=\"progress-bar-dot\" :style=\"{ left: progressPercent + '%' }\"></div>\n        </div>\n\n        <div class=\"controls\">\n          <button @click=\"previousSong\" class=\"btn\" style=\"font-size: 15px; margin-right: -10px;  color: #e2dfd6;\">⏮</button>\n          <button @click=\"toggleAudio\" class=\"btn\" style=\"font-size: 20px; background: #e2dfd6; border-radius: 100px; width:45px; color: #687160;\">\n            {{ isPlaying ? '⏸' : '▸' }}\n          </button>\n          <button @click=\"nextSong\" class=\"btn\" style=\"font-size: 15px; margin-left: -10px; color: #e2dfd6;\">⏭</button>\n        </div>\n      </div>\n    </div>\n\n    <div v-if=\"isMinimized\" style=\"position: fixed; right: 40px; bottom: 40px;\">\n      <button @click=\"toggleAudio\" class=\"btn minimized-btn\" :class=\"{ 'minimized': isMinimized }\">\n        {{ isPlaying ? '⏸' : '▸' }}\n      </button>\n    </div>\n    \n    <div class=\"minimize-maximize-btn\" @click=\"toggleMinimized\" :class=\"{ 'minimized': isMinimized }\"></div>\n  </div>\n</template>\n\n<script>\n//import AOS from \"aos\";\n//import \"aos/dist/aos.css\";\n\nimport HomePage from './components/HomePage.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    HomePage\n  }\n}\n</script>\n\n<style scoped>\n#app {\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n}\n\n/* Use ::v-deep to style child components */\n::v-deep .navbar-brand {\n  font-size: medium;\n  font-family: monospace;\n  letter-spacing: 1px;\n  color: #e2dfd6;\n  transition: all 0.3s ease;\n}\n\n::v-deep .navbar-brand:hover {\n  font-size: medium;\n  font-family: monospace;\n  letter-spacing: 1px;\n  font-weight: bold;\n  color: #e2dfd6;\n  transform: scale(1.3);\n}\n</style>"],"mappings":"AAkDA;AACA;;AAEA,OAAOA,QAAO,MAAO,2BAA0B;AAE/C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}