{"ast":null,"code":"import AOS from \"aos\";\nimport \"aos/dist/aos.css\";\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      title: \"\",\n      words: [\"Portfolio.\"],\n      currentWordIndex: 0,\n      offset: 0,\n      forwards: true,\n      skipCount: 0,\n      skipDelay: 15,\n      speed: 200\n    };\n  },\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n    this.wordFlick();\n    AOS.init({\n      duration: 1000,\n      once: true\n    });\n  },\n  methods: {\n    handleScroll() {\n      const scrollTop = event.target.scrollTop;\n      this.textMarginRight = `${scrollTop * 2}px`;\n    },\n    wordFlick() {\n      setInterval(() => {\n        const currentWord = this.words[this.currentWordIndex];\n        if (this.forwards) {\n          if (this.offset >= currentWord.length) {\n            this.skipCount++;\n            if (this.skipCount === this.skipDelay) {\n              this.forwards = false;\n              this.skipCount = 0;\n            }\n          } else {\n            this.offset++;\n          }\n        } else {\n          if (this.offset === 0) {\n            this.forwards = true;\n            this.currentWordIndex = (this.currentWordIndex + 1) % this.words.length;\n          } else {\n            this.offset--;\n          }\n        }\n        this.title = currentWord.substring(0, this.offset);\n      }, this.speed);\n    }\n  },\n  updated() {\n    AOS.refresh();\n  }\n};","map":{"version":3,"names":["AOS","name","data","title","words","currentWordIndex","offset","forwards","skipCount","skipDelay","speed","mounted","window","addEventListener","handleScroll","wordFlick","init","duration","once","methods","scrollTop","event","target","textMarginRight","setInterval","currentWord","length","substring","updated","refresh"],"sources":["/Users/annaphens/portfolio/src/components/HomePage.vue"],"sourcesContent":["<template>\n  <div class=\"home\" id=\"home\" @scroll=\"handleScroll\" style=\"overflow-y: scroll;\">\n    <div class=\"portfolio-page\">\n      <h2 class=\"title-overlay\">{{ title }}</h2>\n      <img :src=\"require(`@/assets/images/flower2.jpg`)\" class=\"background\" />\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport AOS from \"aos\";\nimport \"aos/dist/aos.css\";\n\nexport default {\n  name: 'HomePage',\n  data(){\n    return{\n      title: \"\",\n      words: [\"Portfolio.\"],\n      currentWordIndex: 0,\n      offset: 0,\n      forwards: true,\n      skipCount: 0,\n      skipDelay: 15,\n      speed: 200\n    }\n  },\n\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n    this.wordFlick();\n    AOS.init({\n      duration: 1000,\n      once: true,\n    });\n  },\n\n  methods: {\n    handleScroll() {\n      const scrollTop = event.target.scrollTop;\n      this.textMarginRight = `${scrollTop * 2}px`;\n    },\n    wordFlick() {\n      setInterval(() => {\n        const currentWord = this.words[this.currentWordIndex];\n        \n        if (this.forwards) {\n          if (this.offset >= currentWord.length) {\n            this.skipCount++;\n            if (this.skipCount === this.skipDelay) {\n              this.forwards = false;\n              this.skipCount = 0;\n            }\n          } else {\n            this.offset++;\n          }\n        } else {\n          if (this.offset === 0) {\n            this.forwards = true;\n            this.currentWordIndex = (this.currentWordIndex + 1) % this.words.length;\n          } else {\n            this.offset--;\n          }\n        }\n        this.title = currentWord.substring(0, this.offset);\n      }, this.speed);\n    },\n  },\n  updated() {\n    AOS.refresh();\n  },\n};\n</script>\n\n<style scoped>\n.home{\n  display: flex;\n  justify-content: flex-start;\n  height: 100vh;\n  background: #e2dfd6;\n  overflow-y: hidden;\n}\n\n.image-container {\n  position: relative;\n  width: 100%;\n  height: 100vh;\n}\n\n.title-overlay {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  font-size: 6em;\n  color: #e2dfd6;\n  font-family: 'EB Garamond';\n  font-weight: 900;\n  text-align: center;\n  z-index: 1;\n  text-shadow: 1px 1px 3px #00000037;\n}\n\n.background {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100vh;\n  object-fit: cover;\n}\n\n.scrollable-content {\n  height: 100vh;\n  width: 100%;\n  overflow-y: scroll;\n}\n</style>\n"],"mappings":"AAWA,OAAOA,GAAE,MAAO,KAAK;AACrB,OAAO,kBAAkB;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,CAAC,YAAY,CAAC;MACrBC,gBAAgB,EAAE,CAAC;MACnBC,MAAM,EAAE,CAAC;MACTC,QAAQ,EAAE,IAAI;MACdC,SAAS,EAAE,CAAC;MACZC,SAAS,EAAE,EAAE;MACbC,KAAK,EAAE;IACT;EACF,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACRC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACC,YAAY,CAAC;IACpD,IAAI,CAACC,SAAS,CAAC,CAAC;IAChBf,GAAG,CAACgB,IAAI,CAAC;MACPC,QAAQ,EAAE,IAAI;MACdC,IAAI,EAAE;IACR,CAAC,CAAC;EACJ,CAAC;EAEDC,OAAO,EAAE;IACPL,YAAYA,CAAA,EAAG;MACb,MAAMM,SAAQ,GAAIC,KAAK,CAACC,MAAM,CAACF,SAAS;MACxC,IAAI,CAACG,eAAc,GAAI,GAAGH,SAAQ,GAAI,CAAC,IAAI;IAC7C,CAAC;IACDL,SAASA,CAAA,EAAG;MACVS,WAAW,CAAC,MAAM;QAChB,MAAMC,WAAU,GAAI,IAAI,CAACrB,KAAK,CAAC,IAAI,CAACC,gBAAgB,CAAC;QAErD,IAAI,IAAI,CAACE,QAAQ,EAAE;UACjB,IAAI,IAAI,CAACD,MAAK,IAAKmB,WAAW,CAACC,MAAM,EAAE;YACrC,IAAI,CAAClB,SAAS,EAAE;YAChB,IAAI,IAAI,CAACA,SAAQ,KAAM,IAAI,CAACC,SAAS,EAAE;cACrC,IAAI,CAACF,QAAO,GAAI,KAAK;cACrB,IAAI,CAACC,SAAQ,GAAI,CAAC;YACpB;UACF,OAAO;YACL,IAAI,CAACF,MAAM,EAAE;UACf;QACF,OAAO;UACL,IAAI,IAAI,CAACA,MAAK,KAAM,CAAC,EAAE;YACrB,IAAI,CAACC,QAAO,GAAI,IAAI;YACpB,IAAI,CAACF,gBAAe,GAAI,CAAC,IAAI,CAACA,gBAAe,GAAI,CAAC,IAAI,IAAI,CAACD,KAAK,CAACsB,MAAM;UACzE,OAAO;YACL,IAAI,CAACpB,MAAM,EAAE;UACf;QACF;QACA,IAAI,CAACH,KAAI,GAAIsB,WAAW,CAACE,SAAS,CAAC,CAAC,EAAE,IAAI,CAACrB,MAAM,CAAC;MACpD,CAAC,EAAE,IAAI,CAACI,KAAK,CAAC;IAChB;EACF,CAAC;EACDkB,OAAOA,CAAA,EAAG;IACR5B,GAAG,CAAC6B,OAAO,CAAC,CAAC;EACf;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}