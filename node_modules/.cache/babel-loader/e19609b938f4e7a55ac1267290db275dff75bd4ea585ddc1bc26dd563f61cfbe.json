"],"sourcesContent":["<template>\n  <div class=\"home\" id=\"home\" @scroll=\"handleScroll\" style=\"overflow-y: scroll;\">\n    <div>\n      <nav class=\"navbar fixed-top navbar-expand-lg\" style=\"\">\n        <div class=\"container-fluid\">\n          <a href=\"#home\">\n            <img src=\"../assets/logo.png\" style=\"width:50px;align-items: center;\">\n          </a>\n          <a class=\"navbar-brand\" href=\"#about\">ABOUT</a>\n          <a class=\"navbar-brand\" href=\"#background\">BACKGROUND</a>\n          <a class=\"navbar-brand\" href=\"#projects\">PROJECTS</a>\n          <a class=\"navbar-brand\" href=\"#contacts\">CONTACTS</a>\n          <p></p>\n        </div>\n      </nav>\n    </div>\n\n    <div>\n      <img :src=\"require(`@/assets/images/flower2.jpg`)\" alt=\"Book Cover\" class=\"background\" />\n    </div>\n    \n    <div class=\"card-container\">\n      <div v-if=\"!isMinimized\">\n        <div class=\"card d-flex justify-content-center align-items-center shadow\">\n          <p class=\"music-title\">{{ Object.keys(songTitles[currentSongIndex])[0] }}</p>\n          <p class=\"music-author\">{{ Object.values(songTitles[currentSongIndex])[0] }}</p>\n\n          <div class=\"progress-container\">\n            <div class=\"progress-bar\" ref=\"progressBar\"></div>\n            <div class=\"progress-bar-dot\" :style=\"{ left: progressPercent + '%' }\"></div>\n          </div>\n\n          <div class=\"controls\" style=\"margin-bottom: 5px;\">\n            <button @click=\"previousSong\" class=\"btn\" style=\"font-size: 1.4vw; margin-right: -10px;  color: #e2dfd6;\">⏮</button>\n            <button @click=\"toggleAudio\" class=\"btn\" style=\"font-size: 1.5vw; background: #e2dfd6; border-radius: 100px; width:45px; color: #687160;\">\n              {{ isPlaying ? '⏸' : '▸' }}\n            </button>\n            <button @click=\"nextSong\" class=\"btn\" style=\"font-size: 1.4vw; margin-left: -10px; color: #e2dfd6;\">⏭</button>\n          </div>\n        </div>\n      </div>\n\n      <div v-if=\"isMinimized\">\n        <button @click=\"toggleAudio\" class=\"btn minimized-btn\" style=\"font-size: 2vw; background: #e2dfd6; border-radius: 50%; color: #687160;\">\n          {{ isPlaying ? '⏸' : '▸' }}\n        </button>\n      </div>\n      \n      <div class=\"minimize-maximize-btn\" @click=\"toggleMinimized\">\n        <span v-if=\"isMinimized\" style=\"font-size: 2vw; margin: 10px\"><b>+</b></span>\n        <span v-else style=\"font-size: 2vw; margin: 10px\"><b>-</b></span>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport AOS from \"aos\";\nimport \"aos/dist/aos.css\";\n\nexport default {\n  name: 'HomePage',\n  data(){\n    return{\n      audio: null,\n      isPlaying: false,\n      currentSongIndex: 0,\n      progressPercent: 0,\n      isMinimized: false,\n      songTitles: [ \n        {'Save A Prayer': 'Duran Duran'},\n        {'You Needed Me': 'Anne Murray'},\n        {\"We're All Alone\": 'Rita Coolidge'},\n        {'One Day In Your Life': 'Michael Jackson'},\n        {\"'74-'75\": 'The Connells'},\n        {'House Of The Rising Sun': 'The Animals'},\n        {'One More Night': 'Phil Collins'},\n        {'Daydream': 'Wallace Collection'},\n        {'Sailing': 'Christopher Cross'},\n        {'Three Times A Lady': 'Commodores'},\n        {'Broken Wings': 'Mr. Mister'},\n        {\"It's Over\": 'Level 42'},\n        {\"Don't Wanna Lose You\": 'Gloria Estefan'},\n        {'Every Time You Go Away': 'Paul Young'},\n        {'Against All Odds': 'Phil Collins'},\n        {'Forever Young': 'Alphaville'},\n        {'Every Breath You Take': 'The Police'},\n        {'A Groovy Kind Of Love': 'Phil Collins'}\n      ],\n      songs: [\n        require('@/assets/songs/saveAPrayer.mp3'),\n        require('@/assets/songs/youNeededMe.mp3'),\n        require('@/assets/songs/wereAllAlone.mp3'),\n        require('@/assets/songs/oneDayInYourLife.mp3'),\n        require('@/assets/songs/7475.mp3'),\n        require('@/assets/songs/houseOfTheRisingSun.mp3'),\n        require('@/assets/songs/oneMoreNight.mp3'),\n        require('@/assets/songs/daydream.mp3'),\n        require('@/assets/songs/sailing.mp3'),\n        require('@/assets/songs/threeTimesALady.mp3'),\n        require('@/assets/songs/brokenWings.mp3'),\n        require('@/assets/songs/itsOver.mp3'),\n        require('@/assets/songs/dontWannaLoseYou.mp3'),\n        require('@/assets/songs/everyTimeYouGoAway.mp3'),\n        require('@/assets/songs/againstAllOdds.mp3'),\n        require('@/assets/songs/foreverYoung.mp3'),\n        require('@/assets/songs/everyBreathYouTake.mp3'),\n        require('@/assets/songs/aGroovyKindOfLove.mp3')\n      ]\n    }\n  },\n\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n    this.setupAudio();\n    AOS.init({\n      duration: 1000,\n      once: true,\n    });\n  },\n\n  methods: {\n    setupAudio() {\n      this.audio = new Audio(this.songs[this.currentSongIndex]);\n      this.audio.loop = false;\n      this.audio.addEventListener('ended', this.nextSong);\n      this.audio.addEventListener('timeupdate', this.updateProgress);\n    },\n    toggleAudio() {\n      this.isPlaying\n        ? this.audio.pause()\n        : this.audio.play().catch(error => {\n          console.warn(\"Play failed:\", error);\n        });\n      this.isPlaying = !this.isPlaying;\n    },\n    nextSong() {\n      this.currentSongIndex = (this.currentSongIndex + 1) % this.songs.length;\n      this.changeSong();\n    },\n    previousSong() {\n      this.currentSongIndex = (this.currentSongIndex - 1 + this.songs.length) % this.songs.length;\n      this.changeSong();\n    },\n    changeSong() {\n      this.audio.src = this.songs[this.currentSongIndex];\n      if (this.isPlaying) {\n        this.audio.play().catch(error => {\n          console.warn(\"Play failed:\", error);\n        });\n      }\n    },\n    updateProgress() {\n      const percent = (this.audio.currentTime / this.audio.duration) * 100;\n      if (!isNaN(percent)) {\n        this.progressPercent = percent;\n      }\n    },\n    seek(event) {\n      const progressBar = this.$refs.progressBar;\n      const clickPosition = (event.clientX - progressBar.offsetLeft) / progressBar.offsetWidth;\n      const newTime = clickPosition * this.audio.duration;\n      this.audio.currentTime = newTime;\n    },\n    handleScroll() {\n      const scrollTop = event.target.scrollTop;\n      this.textMarginRight = `${scrollTop * 2}px`;\n    },\n    toggleMinimized() {\n      this.isMinimized = !this.isMinimized;\n    }\n  },\n\n  beforeUnmount() {\n    this.audio.removeEventListener('ended', this.nextSong);\n    this.audio.pause();\n  },\n\n  updated() {\n    AOS.refresh();\n  },\n};\n</script>\n\n<style scoped>\n@import url('https://fonts.googleapis.com/css2?family=EB+Garamond:ital,wght@0,400..800;1,400..800&family=Montserrat+Alternates:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Work+Sans:ital,wght@0,100..900;1,100..900&display=swap');\n\n.home{\n  display: flex;\n  justify-content: flex-start;\n  height: 100vh;\n  background: #e2dfd6;\n  overflow-y: hidden;\n}\n\n.background {\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100vh;\n    object-fit: cover;\n  }\n\n.scrollable-content {\n  height: 100vh;\n  width: 100%;\n  overflow-y: scroll;\n}\n\n.navbar-brand{\n  font-size: medium;\n  font-family:monospace;\n  letter-spacing: 1px;\n  color: #e2dfd6;\n  transition: all 0.3s ease;\n}\n\n.navbar-brand:hover{\n  font-size: medium;\n  font-family: monospace;\n  letter-spacing: 1px;\n  font-weight: bold;\n  color: #e2dfd6;\n  transform: scale(1.3);\n}\n\n.controls {\n  margin-top: 20px;\n}\nbutton {\n  margin: 0 5px;\n}\n\n.music-title{\n  font-family: 'Arial Black';\n  font-weight: 700;\n  font-size: 1.7vw;\n  color: #e2dfd6;\n  text-align: center;\n}\n\n.music-author{\n  font-family: Arial, Helvetica, sans-serif;\n  font-weight: normal;\n  font-size: 1.2vw;\n  margin-top: -5%;\n  color: #e2dfd6;\n  text-align: center;\n}\n\n.card-container {\n  position: fixed;\n  bottom: 5%;\n  right: 7%;\n  z-index: 1;\n  width: 25%;\n  max-width: 450px;\n}\n\n.card {\n  border: none;\n  border-radius: 15px;\n  background: #687160;\n  height: 35vw;\n  max-height: 12em;\n}\n\n.card.minimized {\n  width: 60px;\n  height: 60px;\n  padding: 0;\n}\n\n.minimize-maximize-btn {\n  position: absolute;\n  top: 0;\n  right: 0;\n  border-radius: 50%;\n  background-color: #e2dfd6;\n  color: #687160;\n  cursor: pointer;\n}\n\n.minimized-btn {\n  font-size: 1vw;\n  background: #687160;\n  border-radius: 50%;\n  width: 50px;\n  height: 50px;\n  color: #687160;\n  margin-top: 10px;\n  margin-left: 5px;\n}\n\n.progress-container {\n  align-items: center;\n  position: relative;\n  width: 80%;\n  margin-top: 1vh;\n}\n\n.progress-bar {\n  height: 0.2vw;\n  background-color: #e2dfd6;\n  width: 100%;\n  position: relative;\n  z-index: 0;\n}\n\n.progress-bar-dot {\n  position: absolute;\n  top: 50%;\n  width: 10px;\n  height: 10px;\n  background-color: #e2dfd6;\n  border-radius: 50%;\n  border: 0.2vw solid #e2dfd6;\n  transform: translate(-50%, -50%);\n  z-index: 1;\n}\n</style>\n"],"mappings":";OAMiBA,UAAwB;mBANzC;;EAqBSC,KAAK,EAAC;AAAgB;;EArB/BC,GAAA;AAAA;;EAuBaD,KAAK,EAAC;AAA8D;;EACpEA,KAAK,EAAC;AAAa;;EACnBA,KAAK,EAAC;AAAc;;EAElBA,KAAK,EAAC;AAAoB;;EACxBA,KAAK,EAAC,cAAc;EAACE,GAAG,EAAC;;;EAI3BF,KAAK,EAAC,UAAU;EAACG,KAA2B,EAA3B;IAAA;EAAA;;;EAhChCF,GAAA;AAAA;;EAAAA,GAAA;EAiDiCE,KAAoC,EAApC;IAAA;IAAA;EAAA;;;EAjDjCF,GAAA;EAkDqBE,KAAoC,EAApC;IAAA;IAAA;EAAA;;;uBAjDnBC,mBAAA,CAoDM;IApDDJ,KAAK,EAAC,MAAM;IAACK,EAAE,EAAC,MAAM;IAAEC,QAAM,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,YAAA,IAAAD,QAAA,CAAAC,YAAA,IAAAF,IAAA,CAAY;IAAEL,KAA2B,EAA3B;MAAA;IAAA;gCADrDQ,kBAAA,mMAAAZ,UAAA,uZAiBIa,mBAAA,CAEM,cADJA,mBAAA,CAAyF;IAAnFC,GAAG,EAAEC,OAAO;IAAiCC,GAAG,EAAC,YAAY;IAACf,KAAK,EAAC;0BAlBhFgB,UAAA,E,GAqBIJ,mB