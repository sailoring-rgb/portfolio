nnaLoseYou.mp3'),\n        require('@/assets/songs/everyTimeYouGoAway.mp3'),\n        require('@/assets/songs/againstAllOdds.mp3'),\n        require('@/assets/songs/foreverYoung.mp3'),\n        require('@/assets/songs/everyBreathYouTake.mp3'),\n        require('@/assets/songs/aGroovyKindOfLove.mp3')\n      ]\n    }\n  },\n\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n    this.setupAudio();\n    AOS.init({\n      duration: 1000,\n      once: true,\n    });\n  },\n\n  methods: {\n    setupAudio() {\n      this.audio = new Audio(this.songs[this.currentSongIndex]);\n      this.audio.loop = false;\n      this.audio.addEventListener('ended', this.nextSong);\n      this.audio.addEventListener('timeupdate', this.updateProgress);\n    },\n    toggleAudio() {\n      this.isPlaying\n        ? this.audio.pause()\n        : this.audio.play().catch(error => {\n          console.warn(\"Play failed:\", error);\n        });\n      this.isPlaying = !this.isPlaying;\n    },\n    nextSong() {\n      this.currentSongIndex = (this.currentSongIndex + 1) % this.songs.length;\n      this.changeSong();\n    },\n    previousSong() {\n      this.currentSongIndex = (this.currentSongIndex - 1 + this.songs.length) % this.songs.length;\n      this.changeSong();\n    },\n    changeSong() {\n      this.audio.src = this.songs[this.currentSongIndex];\n      if (this.isPlaying) {\n        this.audio.play().catch(error => {\n          console.warn(\"Play failed:\", error);\n        });\n      }\n    },\n    updateProgress() {\n      const percent = (this.audio.currentTime / this.audio.duration) * 100;\n      if (!isNaN(percent)) {\n        this.progressPercent = percent;\n      }\n    },\n    seek(event) {\n      const progressBar = this.$refs.progressBar;\n      const clickPosition = (event.clientX - progressBar.offsetLeft) / progressBar.offsetWidth;\n      const newTime = clickPosition * this.audio.duration;\n      this.audio.currentTime = newTime;\n    },\n    handleScroll() {\n      const scrollTop = event.target.scrollTop;\n      this.textMarginRight = `${scrollTop * 2}px`;\n    },\n  },\n\n  beforeUnmount() {\n    this.audio.removeEventListener('ended', this.nextSong);\n    this.audio.pause();\n  },\n\n  updated() {\n    AOS.refresh();\n  },\n};\n</script>\n\n<style scoped>\n@import url('https://fonts.googleapis.com/css2?family=EB+Garamond:ital,wght@0,400..800;1,400..800&family=Montserrat+Alternates:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Work+Sans:ital,wght@0,100..900;1,100..900&display=swap');\n\n.home{\n  display: flex;\n  justify-content: flex-start;\n  height: 100vh;\n  background: #e2dfd6;\n  overflow-y: hidden;\n}\n\n.background {\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100vh;\n    object-fit: cover;\n  }\n\n.scrollable-content {\n  height: 100vh;\n  width: 100%;\n  overflow-y: scroll;\n}\n\n.navbar-brand{\n  font-size: medium;\n  font-family:monospace;\n  letter-spacing: 1px;\n  color: #e2dfd6;\n  transition: all 0.3s ease;\n}\n\n.navbar-brand:hover{\n  font-size: medium;\n  font-family: monospace;\n  letter-spacing: 1px;\n  font-weight: bold;\n  color: #e2dfd6;\n  transform: scale(1.3);\n}\n\n.controls {\n  margin-top: 20px;\n}\nbutton {\n  margin: 0 5px;\n}\n\n.music-title{\n  font-family: 'Arial Black';\n  font-weight: 700;\n  font-size: 1.7vw;\n  color: #e2dfd6;\n  text-align: center;\n}\n\n.music-author{\n  font-family: Arial, Helvetica, sans-serif;\n  font-weight: normal;\n  font-size: 1.2vw;\n  margin-top: -5%;\n  color: #e2dfd6;\n  text-align: center;\n}\n\n.card {\n  position: fixed;\n  bottom: 5%;\n  right: 7%;\n  border: none;\n  border-radius: 15px;\n  background: #687160;\n  height: 35vw;\n  max-height: 12em;\n  width: 25%;\n  max-width: 450px;\n  z-index: 1;\n}\n\n.progress-container {\n  position: relative;\n  width: 80%;\n  margin-top: 1vh;\n}\n\n.progress-bar {\n  height: 0.2vw;