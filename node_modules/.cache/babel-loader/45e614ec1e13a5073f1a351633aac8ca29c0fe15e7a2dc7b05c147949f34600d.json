{"ast":null,"code":"import AOS from \"aos\";\nimport \"aos/dist/aos.css\";\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      title: \"\",\n      words: [\"Portfolio.\"],\n      currentWordIndex: 0,\n      offset: 0,\n      forwards: true,\n      skipCount: 0,\n      skipDelay: 15,\n      speed: 200\n    };\n  },\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n    this.wordFlick();\n    AOS.init({\n      duration: 1000,\n      once: true\n    });\n  },\n  methods: {\n    handleScroll() {\n      const scrollTop = event.target.scrollTop;\n      this.textMarginRight = `${scrollTop * 2}px`;\n    },\n    wordFlick() {\n      setInterval(() => {\n        const currentWord = this.words[this.currentWordIndex];\n        if (this.forwards) {\n          if (this.offset >= currentWord.length) {\n            this.skipCount++;\n            if (this.skipCount === this.skipDelay) {\n              this.forwards = false;\n              this.skipCount = 0;\n            }\n          } else {\n            this.offset++;\n          }\n        } else {\n          if (this.offset === 0) {\n            this.forwards = true;\n            this.currentWordIndex = (this.currentWordIndex + 1) % this.words.length;\n          } else {\n            this.offset--;\n          }\n        }\n\n        // Update title with the substring up to `offset`\n        this.title = currentWord.substring(0, this.offset);\n      }, this.speed);\n    }\n  },\n  beforeUnmount() {\n    this.audio.removeEventListener('ended', this.nextSong);\n    this.audio.pause();\n  },\n  updated() {\n    AOS.refresh();\n  }\n};","map":{"version":3,"names":["AOS","name","data","title","words","currentWordIndex","offset","forwards","skipCount","skipDelay","speed","mounted","window","addEventListener","handleScroll","wordFlick","init","duration","once","methods","scrollTop","event","target","textMarginRight","setInterval","currentWord","length","substring","beforeUnmount","audio","removeEventListener","nextSong","pause","updated","refresh"],"sources":["/Users/annaphens/portfolio/src/components/HomePage.vue"],"sourcesContent":["<template>\n  <div class=\"home\" id=\"home\" @scroll=\"handleScroll\" style=\"overflow-y: scroll;\">\n    <div class=\"portfolio-page\">\n      <h2 class=\"title-overlay\">{{ title }}</h2>\n      <img :src=\"require(`@/assets/images/flower2.jpg`)\" class=\"background\" />\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport AOS from \"aos\";\nimport \"aos/dist/aos.css\";\n\nexport default {\n  name: 'HomePage',\n  data(){\n    return{\n      title: \"\",\n      words: [\"Portfolio.\"],\n      currentWordIndex: 0,\n      offset: 0,\n      forwards: true,\n      skipCount: 0,\n      skipDelay: 15,\n      speed: 200\n    }\n  },\n\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n    this.wordFlick();\n    AOS.init({\n      duration: 1000,\n      once: true,\n    });\n  },\n\n  methods: {\n    handleScroll() {\n      const scrollTop = event.target.scrollTop;\n      this.textMarginRight = `${scrollTop * 2}px`;\n    },\n    wordFlick() {\n      setInterval(() => {\n        const currentWord = this.words[this.currentWordIndex];\n        \n        if (this.forwards) {\n          if (this.offset >= currentWord.length) {\n            this.skipCount++;\n            if (this.skipCount === this.skipDelay) {\n              this.forwards = false;\n              this.skipCount = 0;\n            }\n          } else {\n            this.offset++;\n          }\n        } else {\n          if (this.offset === 0) {\n            this.forwards = true;\n            this.currentWordIndex = (this.currentWordIndex + 1) % this.words.length;\n          } else {\n            this.offset--;\n          }\n        }\n\n        // Update title with the substring up to `offset`\n        this.title = currentWord.substring(0, this.offset);\n      }, this.speed);\n    },\n  },\n\n  beforeUnmount() {\n    this.audio.removeEventListener('ended', this.nextSong);\n    this.audio.pause();\n  },\n\n  updated() {\n    AOS.refresh();\n  },\n};\n</script>\n\n<style scoped>\n@import url('https://fonts.googleapis.com/css2?family=EB+Garamond:ital,wght@0,400..800;1,400..800&family=Montserrat+Alternates:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Work+Sans:ital,wght@0,100..900;1,100..900&display=swap');\n\n.home{\n  display: flex;\n  justify-content: flex-start;\n  height: 100vh;\n  background: #e2dfd6;\n  overflow-y: hidden;\n}\n\n.image-container {\n  position: relative;\n  width: 100%;\n  height: 100vh;\n}\n\n.title-overlay {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  font-size: 6em;\n  color: #e2dfd6;\n  font-family: 'EB Garamond';\n  font-weight: 900;\n  text-align: center;\n  z-index: 1;\n  text-shadow: 1px 1px 3px #00000037;\n}\n\n.background {\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100vh;\n    object-fit: cover;\n  }\n\n.scrollable-content {\n  height: 100vh;\n  width: 100%;\n  overflow-y: scroll;\n}\n\n.controls {\n  margin-top: 20px;\n}\nbutton {\n  margin: 0 5px;\n}\n\n.music-title{\n  font-family: 'Arial Black';\n  font-weight: 700;\n  font-size: 18px;\n  color: #e2dfd6;\n  text-align: center;\n}\n\n.music-author{\n  font-family: Arial, Helvetica, sans-serif;\n  font-weight: normal;\n  font-size: 12px;\n  margin-top: -5%;\n  color: #e2dfd6;\n  text-align: center;\n}\n\n.card-container {\n  position: fixed;\n  bottom: 5%;\n  right: 7%;\n  z-index: 1;\n  width: 20%;\n  min-width: 150px;\n  max-width: 350px;\n}\n\n.card {\n  border: none;\n  border-radius: 15px;\n  background: #687160;\n  min-height: 170px;\n}\n\n.card.minimized {\n  width: 60px;\n  height: 60px;\n  padding: 0;\n}\n\n.minimize-maximize-btn.minimized::after {\n  position: fixed;\n  content: '+';\n  right: 40px;\n  font-size: 1.0rem;\n  font-weight: bold;\n  border-radius: 50%;\n  background: #687160;\n  color: #e2dfd6;\n  width: 20px;\n  height: 20px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  transform: translateY(-250%);\n  cursor: pointer;\n}\n\n.minimize-maximize-btn:not(.minimized)::after {\n  position: absolute;\n  top: 5px;\n  right: 5px;\n  content: '-';\n  font-size: 1.0rem;\n  font-weight: bold;\n  border-radius: 50%;\n  background: #e2dfd6;\n  color: #687160;\n  width: 20px;\n  height: 20px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  cursor: pointer;\n}\n\n.minimized-btn {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 20px;\n  background: #e2dfd6;\n  border-radius: 50%;\n  color: #687160;\n  width: 45px;\n  height: 45px;\n}\n\n.minimized-btn:hover{\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 20px;\n  background: #e2dfd6;\n  border-radius: 50%;\n  color: #687160;\n  width: 45px;\n  height: 45px;\n}\n\n.progress-container {\n  align-items: center;\n  position: relative;\n  width: 75%;\n  margin-top: 1vh;\n}\n\n.progress-bar {\n  height: 1.5px;\n  background-color: #e2dfd6;\n  width: 100%;\n  position: relative;\n  z-index: 0;\n}\n\n.progress-bar-dot {\n  position: absolute;\n  top: 50%;\n  width: 7px;\n  height: 7px;\n  background-color: #e2dfd6;\n  border-radius: 50%;\n  border: 0.2vw solid #e2dfd6;\n  transform: translate(-50%, -50%);\n  z-index: 1;\n}\n</style>\n"],"mappings":"AAWA,OAAOA,GAAE,MAAO,KAAK;AACrB,OAAO,kBAAkB;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,CAAC,YAAY,CAAC;MACrBC,gBAAgB,EAAE,CAAC;MACnBC,MAAM,EAAE,CAAC;MACTC,QAAQ,EAAE,IAAI;MACdC,SAAS,EAAE,CAAC;MACZC,SAAS,EAAE,EAAE;MACbC,KAAK,EAAE;IACT;EACF,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACRC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACC,YAAY,CAAC;IACpD,IAAI,CAACC,SAAS,CAAC,CAAC;IAChBf,GAAG,CAACgB,IAAI,CAAC;MACPC,QAAQ,EAAE,IAAI;MACdC,IAAI,EAAE;IACR,CAAC,CAAC;EACJ,CAAC;EAED