eft) / progressBar.offsetWidth;\n      const newTime = clickPosition * this.audio.duration;\n      this.audio.currentTime = newTime;\n    },\n\n    toggleMinimized() {\n      this.isMinimized = !this.isMinimized;\n    },\n\n    handleScroll() {\n      const scrollTop = event.target.scrollTop;\n      this.textMarginRight = `${scrollTop * 2}px`;\n    },\n\n    scrollToNextPage() {\n      let nextSectionIndex = this.sections.indexOf(this.currentPage) + 1;\n      if (nextSectionIndex >= this.sections.length) {\n        nextSectionIndex = 0;\n      }\n      this.currentPage = this.sections[nextSectionIndex];\n      this.scrollToPage(this.currentPage);\n    },\n\n    scrollToPage(page){\n      const nextSection = this.$refs[page];\n      nextSection.$el.scrollIntoView({ behavior: 'smooth' });\n    },\n\n    initializeCursor() {\n      this.cursor = this.$refs.cursor;\n      document.body.addEventListener(\"mousemove\", this.onMouseMove);\n\n      class Dot {\n        constructor(index) {\n          this.index = index;\n          this.element = document.createElement(\"span\");\n          this.cursor.appendChild(this.element);\n          this.scale = 1 - 0.05 * index;\n          this.x = 0;\n          this.y = 0;\n        }\n\n        draw(x, y) {\n          this.x += (x - this.x) * 0.15;\n          this.y += (y - this.y) * 0.15;\n          this.element.style.transform = `translate(${this.x}px, ${this.y}px) scale(${this.scale})`;\n        }\n      }\n\n      for (let i = 0; i < this.amount; i++) {\n        this.cursorDots.push(new Dot(i));\n      }\n      console.log(this.cursorDots);\n\n      const animateDots = () => {\n        let { x, y } = this.cursorPosition;\n        this.cursorDots.forEach(dot => {\n          dot.draw(x, y);\n          x += (dot.x - x) * 0.15;\n          y += (dot.y - y) * 0.15;\n        });\n        requestAnimationFrame(animateDots);\n      };\n\n      animateDots();\n    },\n\n    onMouseMove(event) {\n      this.cursorPosition.x = event.clientX