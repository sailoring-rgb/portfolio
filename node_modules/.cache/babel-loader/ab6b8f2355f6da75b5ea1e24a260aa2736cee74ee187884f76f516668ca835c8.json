ongs/everyBreathYouTake.mp3'), require('@/assets/songs/aGroovyKindOfLove.mp3')]\n    };\n  },\n  mounted() {\n    this.setupAudio();\n    AOS.init({\n      duration: 1000,\n      once: true\n    });\n  },\n  methods: {\n    setupAudio() {\n      this.audio = new Audio(this.songs[this.currentSongIndex]);\n      this.audio.loop = false;\n      this.audio.addEventListener('ended', this.nextSong);\n      this.audio.addEventListener('timeupdate', this.updateProgress);\n    },\n    toggleAudio() {\n      this.isPlaying ? this.audio.pause() : this.audio.play().catch(error => {\n        console.warn(\"Play failed:\", error);\n      });\n      this.isPlaying = !this.isPlaying;\n    },\n    nextSong() {\n      this.currentSongIndex = (this.currentSongIndex + 1) % this.songs.length;\n      this.changeSong();\n    },\n    previousSong() {\n      this.currentSongIndex = (this.currentSongIndex - 1 + this.songs.length) % this.songs.length;\n      this.changeSong();\n    },\n    changeSong() {\n      this.audio.src = this.songs[this.currentSongIndex];\n      if (this.isPlaying) {\n        this.audio.play().catch(error => {\n          console.warn(\"Play failed:\", error);\n        });\n      }\n    },\n    updateProgress() {\n      const percent = this.audio.currentTime / this.audio.duration * 100;\n      if (!isNaN(percent)) {\n        this.progressPercent = percent;\n      }\n    },\n    seek(event) {\n      const progressBar = this.$refs.progressBar;\n      const clickPosition = (event.clientX - progressBar.offsetLeft) / progressBar.offsetWidth;\n      const newTime = clickPosition * this.audio.duration;\n      this.audio.currentTime = newTime;\n    }\n  },\n  beforeUnmount() {\n    this.audio.removeEventListener('ended', this.nextSong);\n    this.audio.pause();\n  },\n  updated() {\n    AOS.refresh();\n  }\n};","map":{"version":3,"names":["AOS","name","data","audio","isPlaying","currentSongIndex","progressPercent","songTitles","songs","require","mounted","setupAudio","init","du