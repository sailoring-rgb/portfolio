;\n      this.audio.currentTime = newTime;\n    }\n  },\n\n  beforeUnmount() {\n    this.audio.removeEventListener('ended', this.nextSong);\n    this.audio.pause();\n  },\n\n  updated() {\n    AOS.refresh();\n  },\n};\n</script>\n\n<style scoped>\n@import url('https://fonts.googleapis.com/css2?family=EB+Garamond:ital,wght@0,400..800;1,400..800&family=Montserrat+Alternates:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Work+Sans:ital,wght@0,100..900;1,100..900&display=swap');\n\n.home{\n  display: flex;\n  align-items: center;\n  justify-content: flex-start;\n  position: relative;\n  height: 100vh;\n  background: #e2dfd6;\n  overflow: hidden;\n}\n\n.background {\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    object-fit: cover;\n  }\n\n.navbar-brand{\n  font-size: medium;\n  font-family:monospace;\n  letter-spacing: 1px;\n  color: #e2dfd6;\n  transition: all 0.3s ease;\n}\n\n.navbar-brand:hover{\n  font-size: medium;\n  font-family: monospace;\n  letter-spacing: 1px;\n  font-weight: bold;\n  color: #e2dfd6;\n  transform: scale(1.3);\n}\n\n.controls {\n  margin-top: 20px;\n}\nbutton {\n  margin: 0 5px;\n}\n\n.music-title{\n  font-family: 'Arial Black';\n  font-weight: 700;\n  font-size: 1.7vw;\n  color: #e2dfd6;\n  text-align: center;\n}\n\n.music-author{\n  font-family: Arial, Helvetica, sans-serif;\n  font-weight: normal;\n  font-size: 1.2vw;\n  margin-top: -5%;\n  color: #e2dfd6;\n  text-align: center;\n}\n\n.card{\n  position: absolute;\n  bottom:5%;\n  right: 7%;\n  border: none;\n  border-radius: 15px;\n  background: #687160;\n  height: 35vw;\n  max-height: 12em;\n  width: 25%;\n  max-width: 450px;\n}\n\n.progress-bar {\n  width: 80%;\n  position: relative;\n  height: 2.5px;\n  background-color: #e2dfd6;\n  z-index: -10;\n}\n\n.progress-bar-dot {\n  position: absolute;\n  margin-left: 10%;\n  top: 50%;\n  width: 10px;\n  height: 10px;\n  background-color: #68