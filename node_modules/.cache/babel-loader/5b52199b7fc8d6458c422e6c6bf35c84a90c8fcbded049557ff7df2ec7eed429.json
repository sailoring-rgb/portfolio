{"ast":null,"code":"import { openBlock as _openBlock, createElementBlock as _createElementBlock } from \"vue\";\nconst _hoisted_1 = {\n  class: \"aboutMePage\",\n  id: \"aboutMePage\"\n};\nexport function render(_ctx, _cache, $props, $setup, $data, $options) {\n  return _openBlock(), _createElementBlock(\"div\", _hoisted_1);\n}","map":{"version":3,"names":["class","id","_createElementBlock","_hoisted_1"],"sources":["/Users/annaphens/Desktop/portfolio/src/components/AboutMePage.vue"],"sourcesContent":["<template>\n    <div class=\"aboutMePage\" id=\"aboutMePage\">\n      \n    </div>\n  </template>\n  \n  <script>\n  import AOS from \"aos\";\n  import \"aos/dist/aos.css\";\n  \n  export default {\n    name: 'AboutMePage',\n    data(){\n      return{\n        title: \"\",\n        words: [\"Olá.\", \"Hello.\", \"你好.\", \"नमस्ते.\", \"Hola.\", \"Bonjour.\", \"مرحبًا.\", \"Привет.\", \"Guten tag.\", \"こんにちは.\", \"Merhaba.\", \"Xin Chào.\", \"안녕하세요.\", \"Ciao.\", \"สวัสดี.\"],\n        currentWordIndex: 0,\n        offset: 0,\n        forwards: true,\n        skipCount: 0,\n        skipDelay: 15,\n        speed: 150\n      }\n    },\n  \n    mounted() {\n      this.wordFlick();\n      AOS.init({\n        duration: 1000,\n        once: true,\n      });\n    },\n  \n    methods: {\n      wordFlick() {\n        setInterval(() => {\n          const currentWord = this.words[this.currentWordIndex];\n          \n          if (this.forwards) {\n            if (this.offset >= currentWord.length) {\n              this.skipCount++;\n              if (this.skipCount === 3) {\n                this.forwards = false;\n                this.skipCount = 0;\n              }\n            } else {\n              this.offset++;\n            }\n          } else {\n            if (this.offset === 0) {\n              // Increase the speed of word change\n              this.currentWordIndex = (this.currentWordIndex + 1) % this.words.length;\n              this.forwards = true;\n            } else {\n              this.offset--;\n            }\n          }\n          this.title = currentWord.substring(0, this.offset);\n        }, this.speed); // Speed of typing effect\n      },\n    },\n  \n    updated() {\n      AOS.refresh();\n    },\n  };\n  </script>\n  \n  <style scoped>\n  .home {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center; \n    height: 100vh;\n    background: #e2dfd6;\n    overflow-y: hidden;\n  }\n  \n  .image-container {\n    position: relative;\n    width: 100%;\n    height: 100vh;\n  }\n  \n  .title-overlay {\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -100%);\n    font-size: 6em;\n    color: #e2dfd6;\n    font-family: 'EB Garamond';\n    font-weight: 900;\n    text-align: center;\n    z-index: 1;\n    text-shadow: 1px 1px 3px #00000037;\n  }\n  \n  .welcome-message {\n    position: relative;\n    z-index: 1;\n    font-size: 10px;\n    letter-spacing: 2px;\n    transform: translateY(50%);\n    color: #e2dfd6;\n    font-family: monospace;\n    border: 1px solid #e2dfd6;\n    border-color: #e2dfd6;\n    border-radius: 20px;\n    padding: 5px;\n    padding-left: 40px;\n    padding-right: 40px;\n    margin-top: 10px;\n  }\n  \n  .background {\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100vh;\n    object-fit: cover;\n  }\n  </style>\n  "],"mappings":";;EACSA,KAAK,EAAC,aAAa;EAACC,EAAE,EAAC;;;uBAA5BC,mBAAA,CAEM,OAFNC,UAEM","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}