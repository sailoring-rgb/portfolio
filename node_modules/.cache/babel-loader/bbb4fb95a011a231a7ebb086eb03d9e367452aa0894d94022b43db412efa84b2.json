l', this.handleScroll);\n    this.wordFlick();\n    AOS.init({\n      duration: 1000,\n      once: true,\n    });\n  },\n\n  methods: {\n    handleScroll() {\n      const scrollTop = event.target.scrollTop;\n      this.textMarginRight = `${scrollTop * 2}px`;\n    },\n    wordFlick() {\n      setInterval(() => {\n        const currentWord = this.words[this.currentWordIndex];\n        \n        if (this.forwards) {\n          if (this.offset >= currentWord.length) {\n            this.skipCount++;\n            if (this.skipCount === this.skipDelay) {\n              this.forwards = false;\n              this.skipCount = 0;\n            }\n          } else {\n            this.offset++;\n          }\n        } else {\n          if (this.offset === 0) {\n            this.forwards = true;\n            this.currentWordIndex = (this.currentWordIndex + 1) % this.words.length;\n          } else {\n            this.offset--;\n          }\n        }\n        this.title = currentWord.substring(0, this.offset);\n      }, this.speed);\n    },\n  },\n  updated() {\n    AOS.refresh();\n  },\n};\n</script>\n\n<style scoped>\n.home{\n  display: flex;\n  justify-content: flex-start;\n  height: 100vh;\n  background: #e2dfd6;\n  overflow-y: hidden;\n}\n\n.image-container {\n  position: relative;\n  width: 100%;\n  height: 100vh;\n}\n\n.title-overlay {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  font-size: 6em;\n  color: #e2dfd6;\n  font-family: 'EB Garamond';\n  font-weight: 900;\n  text-align: center;\n  z-index: 1;\n  text-shadow: 1px 1px 3px #00000037;\n}\n\n.background {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100vh;\n  object-fit: cover;\n}\n\n.scrollable-content {\n  height: 100vh;\n  width: 100%;\n  overflow-y: scroll;\n}\n</style>\n"],"mappings":";;EAESA,KAAK,EAAC;AAAgB;;EACrBA,KAAK,EAAC;AAAe;mBAH/B;;uBACEC,mBAAA,CAMM;IANDD,KAAK,EAAC,MAAM;IAACE,EAAE,EAAC,MAAM;IAAEC,QAAM,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,K